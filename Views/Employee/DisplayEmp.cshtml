
@{
    ViewData["Title"] = "DisplayEmp";
}

<h1>DisplayEmp</h1>
<h1>DisplayStudent</h1>

<table class="table">
    <tr>
        <td>Your Name</td>
        <td>
            @ViewBag.Name
        </td>
    </tr>
    <tr>
        <td>Your Roll No</td>
        <td>
            @ViewBag.Id
        </td>
    </tr>
    <tr>
        <td>Gender</td>
        <td>
            @ViewBag.Gender
        </td>
    </tr>
    <tr>
        <td>City</td>
        <td>
            @ViewBag.City
        </td>
    </tr>
    <tr>
        <td>Skills</td>
        <td>
            @foreach(string s in ViewBag.skills)
            {

                 <p>@s</p>                
            }
        </td>
    </tr>
    <tr>
        <td>Department</td>
        <td>
            @ViewBag.department
        </td>
    </tr>

   
</table>
